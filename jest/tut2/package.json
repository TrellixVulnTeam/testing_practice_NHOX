{
   "name":"stuff",
   "scripts":
   {
     "test": "jest --coverage"
   },
   "devDependencies":
   {
     "jest-cli": "^20.0.4",
     "babel-jest":"^15.0.0",
     "jest-runtime":"15.1.0",
     "babel-preset-es2015":"6.14.0",
     "babel-preset-stage-2": "6.13.0"
   },

   "jest":
   {
     "testEnvironment": "node",
     "collectCoverageFrom":
     [
       "src./*js"
     ],
     "coverageThreshold":
     {
       "global":
       {
         "branches": 100,
         "functions": 100,
         "lines": 100,
         "statements":100
       }
     }
   }


}
